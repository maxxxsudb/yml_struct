SELECT
    'INSERT INTO #CHECK_LIST (SQL_COMMAND) VALUES (''' || REPLACE(CMD, '''', '''''') || ''');'
FROM (
    SELECT
        'GRANT "' || ROLE_NAME || '" TO "REVOLUTE"' || CASE WHEN IS_GRANTABLE = 'TRUE' THEN ' WITH ADMIN OPTION' ELSE '' END || ';' AS CMD
    FROM "SYS"."GRANTED_ROLES"
    WHERE GRANTEE = 'REVOLUTE' AND GRANTEE_TYPE = 'USER'
